#include <stdio.h>
#include<string.h>

int main(){
	char a[20];
	scanf("%s",a); 
	//int x,y;
	int x=0;
	int y=0;
	int l=strlen(a);
	for(int i=0;i<l;i++){
         if(a[i]=='z'){ 
			x++;
			}
			else if(a[i]=='o'){
			y++;
			}
			else{
				printf("No");
			}
			}
			
			
			if(y==2*x){
			printf("Yes");
			}
			else{
				printf("No");
			}
			return 0;

}
